{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch Wingspan",
            "type": "chrome",
            "request": "launch",
            "file": "index.html"
        },
        {
            "name": "Launch Wingspan with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://wingspan.192.168.1.111.xip.io/",
            "webRoot": "./",
            "sourceMaps": true,
            "runtimeExecutable": "/Applications/Google Chrome Canary.app/Contents/MacOS/Google Chrome Canary"
        },
        {
            "name": "Attach to Wingspan",
            "type": "chrome",
            "request": "attach",
            "port": 9223,
            "webRoot": "./",
            "sourceMaps": true
        }
    ]
}
